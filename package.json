{
  "name": "livesoccertv-parser",
  "version": "4.0.2",
  "description": "Parse match info from livesoccertv",
  "main": "build.js",
  "scripts": {
    "dev": "ava --watch",
    "test": "xo --fix index.js && npm run build && ava",
    "build": "babel index.js --presets babel-preset-es2015 --out-file build.js",
    "prepublish": "npm test"
  },
  "keywords": [],
  "author": "Pablo Varela",
  "license": "ISC",
  "dependencies": {
    "cheerio": "^1.0.0-rc.2",
    "isomorphic-fetch": "^2.2.1",
    "microfetch": "^1.1.0",
    "moment": "^2.18.1"
  },
  "devDependencies": {
    "ava": "^0.22.0",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-prettier": "^2.3.1",
    "prettier": "^1.7.3",
    "xo": "^0.18.2"
  },
  "xo": {
    "extends": "prettier",
    "plugins": [
      "prettier"
    ],
    "parserOptions": {
      "sourceType": "script"
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "bracketSpacing": true,
          "trailingComma": "none",
          "semi": false
        }
      ],
      "max-len": [
        "error",
        {
          "code": 120,
          "ignoreUrls": true
        }
      ],
      "capitalized-comments": "off",
      "camelcase": "off",
      "no-warning-comments": "off"
    },
    "space": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pablopunk/livesoccertv-parser.git"
  },
  "bugs": {
    "url": "https://github.com/pablopunk/livesoccertv-parser/issues"
  },
  "homepage": "https://github.com/pablopunk/livesoccertv-parser#readme"
}
